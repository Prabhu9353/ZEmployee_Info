{
  "name": "EmployerInfo",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.1",
    "@cap-js/hana": "^1.9.1",
    "@sap/cds": "^8",
    "@sap/hana-client": "^2.25.22",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-employeeinfoapp": "cds watch --open employeeinfoapp/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy EmployerInfo --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-employeedetailsapp": "cds watch --open employeedetailsapp/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "auth": "dummy"
    },
    "db": {
      "kind": "hana"
    }
  },
  "sapux": [
    "app/employeeinfoapp",
    "app/employeedetailsapp",
    "app/employeedetailsapp"
  ]
}
